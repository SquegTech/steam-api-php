{
    "name": "squegtech/steam-api",
    "type": "library",
    "description": "PHP wrapper for the Steam web API",
    "keywords": ["steam", "dota", "api", "csgo", "counter strike"],
    "homepage": "https://github.com/SquegTech/steam-api-php",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Tommy Bolger",
            "email": "tommy.bolger@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "SquegTech\\Steam\\": "src/Steam"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SquegTech\\Steam\\Tests\\": "tests/src/Steam"
        }
    }
}
